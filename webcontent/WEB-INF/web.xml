<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app 
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" 
    "http://java.sun.com/dtd/web-app_2_3.dtd">

 <web-app>
   <display-name>YouTube Video Finder</display-name>
   <context-param>
     <param-name>cacheServer</param-name>
     <param-value>spec20</param-value>
   </context-param>
   <context-param>
     <param-name>cacheServerPort</param-name>
     <param-value>9000</param-value>
   </context-param>
    
   <servlet>
     <servlet-name>youtube</servlet-name>
     <servlet-class>edu.upenn.cis455.youtube.YouTubeSearch</servlet-class> 
   </servlet>
   
   <servlet-mapping>
     <servlet-name>youtube</servlet-name>
     <url-pattern>/youtube</url-pattern>
   </servlet-mapping>

</web-app>
